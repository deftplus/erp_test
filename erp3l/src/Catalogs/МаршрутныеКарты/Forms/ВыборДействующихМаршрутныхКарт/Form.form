    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	ВЫБОР
		КОГДА СправочникМаршрутныеКарты.Ссылка В
				(ВЫБРАТЬ
					ОсновныеМаршрутныеКарты.МаршрутнаяКарта
				ИЗ
					РегистрСведений.ОсновныеМаршрутныеКарты КАК ОсновныеМаршрутныеКарты)
			ТОГДА ИСТИНА
		ИНАЧЕ ЛОЖЬ
	КОНЕЦ КАК Основная,
	СправочникМаршрутныеКарты.Ссылка КАК Ссылка,
	СправочникМаршрутныеКарты.Код КАК Код,
	СправочникМаршрутныеКарты.Наименование КАК Наименование,
	СправочникМаршрутныеКарты.НачалоДействия КАК НачалоДействия,
	СправочникМаршрутныеКарты.КонецДействия КАК КонецДействия,
	СправочникМаршрутныеКарты.Статус КАК Статус,
	СправочникМаршрутныеКарты.Подразделение КАК Подразделение,
	ВЫБОР
		КОГДА СправочникМаршрутныеКарты.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыМаршрутныхКарт.Действует)
				И (&amp;НачалоПроизводства = ДАТАВРЕМЯ(1, 1, 1)
					ИЛИ (СправочникМаршрутныеКарты.НачалоДействия = ДАТАВРЕМЯ(1, 1, 1)
						ИЛИ СправочникМаршрутныеКарты.НачалоДействия &lt;= &amp;НачалоПроизводства)
						И (СправочникМаршрутныеКарты.КонецДействия = ДАТАВРЕМЯ(1, 1, 1)
							ИЛИ СправочникМаршрутныеКарты.КонецДействия >= &amp;НачалоПроизводства))
			ТОГДА ИСТИНА
		ИНАЧЕ ЛОЖЬ
	КОНЕЦ КАК Действует,
	СправочникМаршрутныеКарты.Описание КАК Описание
ИЗ
	Справочник.МаршрутныеКарты КАК СправочникМаршрутныеКарты
ГДЕ
	НЕ СправочникМаршрутныеКарты.ПометкаУдаления
	И СправочникМаршрутныеКарты.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыМаршрутныхКарт.Действует)
	И (&amp;НачалоПроизводства = ДАТАВРЕМЯ(1, 1, 1)
			ИЛИ (СправочникМаршрутныеКарты.НачалоДействия = ДАТАВРЕМЯ(1, 1, 1)
				ИЛИ СправочникМаршрутныеКарты.НачалоДействия &lt;= &amp;НачалоПроизводства)
				И (СправочникМаршрутныеКарты.КонецДействия = ДАТАВРЕМЯ(1, 1, 1)
					ИЛИ СправочникМаршрутныеКарты.КонецДействия >= &amp;НачалоПроизводства))</queryText>
      <mainTable>Catalog.МаршрутныеКарты</mainTable>
