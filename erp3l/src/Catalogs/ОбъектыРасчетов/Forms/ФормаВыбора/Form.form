    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ РАЗЛИЧНЫЕ
	ВЫБОР
		КОГДА РасчетыСКлиентами.Регистратор ЕСТЬ NULL
			ТОГДА РасчетыСПоставщиками.Регистратор
		ИНАЧЕ РасчетыСКлиентами.Регистратор
	КОНЕЦ                                            КАК ОснованиеПлатежа,
	ОбъектыРасчетов.Ссылка                           КАК Ссылка,
	ОбъектыРасчетов.Ссылка                           КАК ОбъектРасчетов,
	ОбъектыРасчетов.Партнер                          КАК Партнер,
	ОбъектыРасчетов.Организация                      КАК Организация,
	ОбъектыРасчетов.Контрагент                       КАК Контрагент,
	ОбъектыРасчетов.Договор                          КАК Договор,
	ОбъектыРасчетов.НаправлениеДеятельности          КАК НаправлениеДеятельности,
	ОбъектыРасчетов.ВалютаВзаиморасчетов             КАК ВалютаВзаиморасчетов,
	ОбъектыРасчетов.ИдентификаторПлатежа             КАК ИдентификаторПлатежа,
	РеестрДокументов.НомерДокументаИБ                КАК Номер,
	РеестрДокументов.ДатаДокументаИБ                 КАК Дата,
	РеестрДокументов.ДатаПервичногоДокумента         КАК ДатаВходящегоДокумента,
	РеестрДокументов.НомерПервичногоДокумента        КАК НомерВходящегоДокумента,
	РеестрДокументов.НаименованиеПервичногоДокумента КАК НаименованиеПервичногоДокумента,
	РеестрДокументов.Сумма                           КАК Сумма,
	РеестрДокументов.Валюта                          КАК Валюта,
	ВЫБОР
		КОГДА РеестрДокументов.ПометкаУдаления
			ТОГДА 0
		КОГДА РеестрДокументов.Проведен
			ТОГДА 2
		ИНАЧЕ 1
	КОНЕЦ                                            КАК Состояние
ИЗ
	Справочник.ОбъектыРасчетов КАК ОбъектыРасчетов
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.РасчетыСКлиентами КАК РасчетыСКлиентами
		ПО (&amp;ТипРасчетов = ЗНАЧЕНИЕ(Перечисление.ТипыРасчетовСПартнерами.РасчетыСКлиентом))
			И (РасчетыСКлиентами.ОбъектРасчетов = ОбъектыРасчетов.Ссылка)
			И (РасчетыСКлиентами.Сумма &lt;> 0
				ИЛИ РасчетыСКлиентами.КОплате &lt;> 0)
			И (ВЫБОР
				КОГДА &amp;ПодборДебиторскойЗадолженности
					ТОГДА РасчетыСКлиентами.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход)
				ИНАЧЕ РасчетыСКлиентами.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Расход)
			КОНЕЦ)
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.РасчетыСПоставщиками КАК РасчетыСПоставщиками
		ПО (&amp;ТипРасчетов = ЗНАЧЕНИЕ(Перечисление.ТипыРасчетовСПартнерами.РасчетыСПоставщиком))
			И (РасчетыСПоставщиками.ОбъектРасчетов = ОбъектыРасчетов.Ссылка)
			И (РасчетыСПоставщиками.Сумма &lt;> 0
				ИЛИ РасчетыСПоставщиками.КОплате &lt;> 0)
			И (ВЫБОР
				КОГДА &amp;ПодборДебиторскойЗадолженности
					ТОГДА РасчетыСПоставщиками.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Расход)
				ИНАЧЕ РасчетыСПоставщиками.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход)
			КОНЕЦ)
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.РеестрДокументов КАК РеестрДокументов
		ПО (РеестрДокументов.Ссылка = ЕСТЬNULL(РасчетыСКлиентами.Регистратор, РасчетыСПоставщиками.Регистратор))
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.ДоговорыКонтрагентов КАК Договоры
		ПО ОбъектыРасчетов.Договор = Договоры.Ссылка
ГДЕ
	(ОбъектыРасчетов.Организация В (&amp;Организация)
			ИЛИ НЕ &amp;ЭтоГоловнаяОрганизация
				И ОбъектыРасчетов.Договор В (&amp;ЦентрализованныеДоговоры)
				И (ОбъектыРасчетов.Организация = &amp;ГоловнаяОрганизация
					ИЛИ ЕСТЬNULL(Договоры.РазрешаетсяПередачаОплатМеждуФилиалами, ЛОЖЬ))
			ИЛИ &amp;ЭтоГоловнаяОрганизация
				И ОбъектыРасчетов.Организация.ГоловнаяОрганизация В (&amp;Организация)
				И ОбъектыРасчетов.Организация.ДопускаютсяВзаиморасчетыЧерезГоловнуюОрганизацию
			ИЛИ &amp;ПоВсемОрганизациям)
	И НЕ ВЫБОР
				КОГДА РасчетыСКлиентами.Регистратор ЕСТЬ NULL
					ТОГДА РасчетыСПоставщиками.Регистратор
				ИНАЧЕ РасчетыСКлиентами.Регистратор
			КОНЕЦ ЕСТЬ NULL
	И (ВЫБОР
				КОГДА РасчетыСКлиентами.Регистратор ЕСТЬ NULL
					ТОГДА РасчетыСПоставщиками.Регистратор
				ИНАЧЕ РасчетыСКлиентами.Регистратор
			КОНЕЦ &lt;> ОбъектыРасчетов.Объект
			ИЛИ &amp;ОтображатьОбъектыРасчетов)
	И ТИПЗНАЧЕНИЯ(РеестрДокументов.Ссылка) В (&amp;ТипыОснований)

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Счет.Ссылка                                     КАК ОснованиеПлатежа,
	ОбъектыРасчетов.Ссылка                          КАК Ссылка,
	ОбъектыРасчетов.Ссылка                          КАК ОбъектРасчетов,
	ОбъектыРасчетов.Партнер                         КАК Партнер,
	ОбъектыРасчетов.Организация                     КАК Организация,
	ОбъектыРасчетов.Контрагент                      КАК Контрагент,
	ОбъектыРасчетов.Договор                         КАК Договор,
	ОбъектыРасчетов.НаправлениеДеятельности         КАК НаправлениеДеятельности,
	ОбъектыРасчетов.ВалютаВзаиморасчетов            КАК ВалютаВзаиморасчетов,
	ОбъектыРасчетов.ИдентификаторПлатежа            КАК ИдентификаторПлатежа,
	Счет.Номер                                      КАК Номер,
	Счет.Дата                                       КАК Дата,
	Счет.Дата                                       КАК ДатаВходящегоДокумента,
	Счет.Номер                                      КАК НомерВходящегоДокумента,
	ОбъектыРасчетов.НаименованиеПервичногоДокумента КАК НаименованиеПервичногоДокумента,
	Счет.СуммаДокумента                             КАК Сумма,
	Счет.Валюта                                     КАК Валюта,
	ВЫБОР 
		КОГДА Счет.Проведен
			ТОГДА 2
			ИНАЧЕ 0
	КОНЕЦ                                           КАК Состояние
ИЗ
	Документ.СчетНаОплатуКлиенту КАК Счет
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.ОбъектыРасчетов КАК ОбъектыРасчетов
		ПО (ВЫБОР
				КОГДА Счет.ДокументОснование ССЫЛКА Документ.ЗаказКлиента
						И Счет.ДокументОснование.ПорядокРасчетов = ЗНАЧЕНИЕ(Перечисление.ПорядокРасчетов.ПоЗаказамНакладным)
					ТОГДА Счет.ДокументОснование
				КОГДА Счет.ДокументОснование.ПорядокРасчетов = ЗНАЧЕНИЕ(Перечисление.ПорядокРасчетов.ПоДоговорамКонтрагентов)
					ТОГДА Счет.Договор
				ИНАЧЕ Счет.ДокументОснование
			КОНЕЦ = ОбъектыРасчетов.Объект)
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.ДоговорыКонтрагентов КАК Договоры
		ПО (ОбъектыРасчетов.Договор = Договоры.Ссылка)
ГДЕ
	НЕ Счет.ПометкаУдаления
	И (ОбъектыРасчетов.Организация В (&amp;Организация)
			ИЛИ НЕ &amp;ЭтоГоловнаяОрганизация
				И ОбъектыРасчетов.Договор В (&amp;ЦентрализованныеДоговоры)
				И (ОбъектыРасчетов.Организация = &amp;ГоловнаяОрганизация
					ИЛИ ЕСТЬNULL(Договоры.РазрешаетсяПередачаОплатМеждуФилиалами, ЛОЖЬ))
			ИЛИ &amp;ЭтоГоловнаяОрганизация
				И ОбъектыРасчетов.Организация.ГоловнаяОрганизация В (&amp;Организация)
				И ОбъектыРасчетов.Организация.ДопускаютсяВзаиморасчетыЧерезГоловнуюОрганизацию
			ИЛИ &amp;ПоВсемОрганизациям)

</queryText>
