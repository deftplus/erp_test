    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ЗаказДавальца.Ссылка КАК Заказ
ПОМЕСТИТЬ ТаблицаЗаказов
ИЗ
	(ВЫБРАТЬ
		ЗаказДавальца.Ссылка КАК Ссылка
	ИЗ
		Документ.ЗаказДавальца КАК ЗаказДавальца
	ГДЕ
		ЗаказДавальца.Договор = &amp;Договор
		И ЗаказДавальца.Организация = &amp;Организация
		И ЗаказДавальца.Валюта = &amp;Валюта
		И ЗаказДавальца.Контрагент = &amp;Контрагент
		И ЗаказДавальца.НалогообложениеНДС = &amp;НалогообложениеНДС
		И ЗаказДавальца.Партнер = &amp;Партнер
		И ЗаказДавальца.ЦенаВключаетНДС = &amp;ЦенаВключаетНДС
		И ЗаказДавальца.ПорядокРасчетов = &amp;ПорядокРасчетов

	ОБЪЕДИНИТЬ ВСЕ

	ВЫБРАТЬ
		ЗНАЧЕНИЕ(Документ.ЗаказДавальца.ПустаяСсылка)) КАК ЗаказДавальца

ИНДЕКСИРОВАТЬ ПО
	Заказ
;
////////////////////////////////////////////////////////////////////////////////
 ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ДанныеРазвернутыеПоНоменклатуре.Договор КАК Договор,
	ДанныеРазвернутыеПоНоменклатуре.ЗаказДавальца КАК ЗаказДавальца,
	ДанныеРазвернутыеПоНоменклатуре.Номенклатура КАК Номенклатура,
	ДанныеРазвернутыеПоНоменклатуре.Характеристика КАК Характеристика
ПОМЕСТИТЬ ДанныеРазвернутыеПоНоменклатуре
ИЗ
	(ВЫБРАТЬ
		Остатки.Договор КАК Договор,
		Остатки.ЗаказДавальца КАК ЗаказДавальца,
		Остатки.Номенклатура КАК Номенклатура,
		Остатки.Характеристика КАК Характеристика,
		Остатки.КОформлениюОстаток КАК КОформлению
	ИЗ
		РегистрНакопления.УслугиДавальцуКОформлению.Остатки(, Договор = &amp;Договор
		И ЗаказДавальца В
			(ВЫБРАТЬ
				ТаблицаЗаказов.Заказ КАК Заказ
			ИЗ
				ТаблицаЗаказов КАК ТаблицаЗаказов)) КАК Остатки

	ОБЪЕДИНИТЬ ВСЕ

	ВЫБРАТЬ
		Движение.Договор,
		Движение.ЗаказДавальца,
		Движение.Номенклатура,
		Движение.Характеристика,
		Движение.КОформлению * ВЫБОР
			КОГДА Движение.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход)
				ТОГДА -1
			ИНАЧЕ 1
		КОНЕЦ
	ИЗ
		РегистрНакопления.УслугиДавальцуКОформлению КАК Движение
	ГДЕ
		Движение.Активность
		И Движение.Регистратор = &amp;Регистратор
		И Движение.Договор = &amp;Договор
		И Движение.ЗаказДавальца В
			(ВЫБРАТЬ
				ТаблицаЗаказов.Заказ КАК Заказ
			ИЗ
				ТаблицаЗаказов КАК ТаблицаЗаказов)) КАК ДанныеРазвернутыеПоНоменклатуре
СГРУППИРОВАТЬ ПО
	ДанныеРазвернутыеПоНоменклатуре.Договор,
	ДанныеРазвернутыеПоНоменклатуре.ЗаказДавальца,
	ДанныеРазвернутыеПоНоменклатуре.Номенклатура,
	ДанныеРазвернутыеПоНоменклатуре.Характеристика
ИМЕЮЩИЕ
	СУММА(ДанныеРазвернутыеПоНоменклатуре.КОформлению) > 0

ИНДЕКСИРОВАТЬ ПО
	Договор,
	ЗаказДавальца
;
////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ДанныеРазвернутыеПоНоменклатуре.Договор КАК Договор,
	ДанныеРазвернутыеПоНоменклатуре.ЗаказДавальца КАК ЗаказДавальца
ПОМЕСТИТЬ Данные
ИЗ
	ДанныеРазвернутыеПоНоменклатуре КАК ДанныеРазвернутыеПоНоменклатуре
СГРУППИРОВАТЬ ПО
	ДанныеРазвернутыеПоНоменклатуре.Договор,
	ДанныеРазвернутыеПоНоменклатуре.ЗаказДавальца

ИНДЕКСИРОВАТЬ ПО
	ЗаказДавальца
;
////////////////////////////////////////////////////////////////////////////////
 ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ЕСТЬNULL(Заказы.Договор, &amp;Договор) КАК Договор,
	ЕСТЬNULL(Заказы.Ссылка, ЗНАЧЕНИЕ(Документ.ЗаказДавальца.ПустаяСсылка)) КАК Ссылка,
	ЕСТЬNULL(Заказы.Дата, ДАТАВРЕМЯ(1, 1, 1)) КАК Дата,
	ЕСТЬNULL(Заказы.Номер, &quot;&quot;) КАК Номер,
	ЕСТЬNULL(Заказы.Партнер, Договоры.Партнер) КАК Партнер,
	ЕСТЬNULL(Заказы.Контрагент, Договоры.Контрагент) КАК Контрагент,
	ЕСТЬNULL(Заказы.Организация, Договоры.Организация) КАК Организация,
	ЕСТЬNULL(Заказы.Склад, ЗНАЧЕНИЕ(Справочник.Склады.ПустаяСсылка)) КАК Склад,
	ЕСТЬNULL(Заказы.Валюта, Договоры.ВалютаВзаиморасчетов) КАК Валюта,
	ЕСТЬNULL(Заказы.Менеджер, Договоры.Менеджер) КАК Менеджер,
	ЕСТЬNULL(Заказы.Статус, ЗНАЧЕНИЕ(Перечисление.СтатусыЗаказовДавальцев.ПустаяСсылка)) КАК Статус,
	ЕСТЬNULL(Заказы.СуммаДокумента, 0) КАК СуммаДокумента,
	ЕСТЬNULL(Заказы.Приоритет, ЗНАЧЕНИЕ(Справочник.Приоритеты.ПустаяСсылка)) КАК Приоритет,
	ЕСТЬNULL(Заказы.ХозяйственнаяОперация, ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.ПустаяСсылка)) КАК ХозяйственнаяОперация,
	ЕСТЬNULL(Заказы.НалогообложениеНДС, ЗНАЧЕНИЕ(Перечисление.ТипыНалогообложенияНДС.ПустаяСсылка)) КАК НалогообложениеНДС,
	ЕСТЬNULL(Заказы.ЦенаВключаетНДС, НЕОПРЕДЕЛЕНО) КАК ЦенаВключаетНДС,
	ЕСТЬNULL(Заказы.ПорядокРасчетов, ЗНАЧЕНИЕ(Перечисление.ПорядокРасчетов.ПустаяСсылка)) КАК ПорядокРасчетов,
	ЕСТЬNULL(Заказы.Комментарий, Договоры.Комментарий) КАК Комментарий,
	ВЫБОР
		КОГДА Заказы.Приоритет В
			(ВЫБРАТЬ ПЕРВЫЕ 1
				Приоритеты.Ссылка КАК Приоритет
			ИЗ
				Справочник.Приоритеты КАК Приоритеты
			УПОРЯДОЧИТЬ ПО
				Приоритеты.РеквизитДопУпорядочивания)
			ТОГДА 0
		КОГДА Заказы.Приоритет В
			(ВЫБРАТЬ ПЕРВЫЕ 1
				Приоритеты.Ссылка КАК Приоритет
			ИЗ
				Справочник.Приоритеты КАК Приоритеты
			УПОРЯДОЧИТЬ ПО
				Приоритеты.РеквизитДопУпорядочивания УБЫВ)
			ТОГДА 2
		ИНАЧЕ 1
	КОНЕЦ КАК КартинкаПриоритета
ИЗ
	Данные КАК Данные
		ЛЕВОЕ СОЕДИНЕНИЕ Документ.ЗаказДавальца КАК Заказы
		ПО Данные.ЗаказДавальца = Заказы.Ссылка
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.ДоговорыКонтрагентов КАК Договоры
		ПО Данные.Договор = Договоры.Ссылка</queryText>
      <mainTable>Document.ЗаказДавальца</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
