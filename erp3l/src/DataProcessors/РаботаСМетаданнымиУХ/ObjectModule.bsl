
Функция NewObject(Тип,
				Параметр1=Неопределено,
				Параметр2=Неопределено,
				Параметр3=Неопределено,
				Параметр4=Неопределено,
				Параметр5=Неопределено) Экспорт 
					
	МассивТипов=Новый Массив;
	
	Если Параметр1<>Неопределено Тогда
		
		МассивТипов.Добавить(Параметр1);
		
	КонецЕсли;
	
	Если Параметр2<>Неопределено Тогда
		
		МассивТипов.Добавить(Параметр2);
		
	КонецЕсли;
	
	Если Параметр3<>Неопределено Тогда
		
		МассивТипов.Добавить(Параметр3);
		
	КонецЕсли;
	
	Если Параметр4<>Неопределено Тогда
		
		МассивТипов.Добавить(Параметр4);
		
	КонецЕсли;
	
	Если Параметр5<>Неопределено Тогда
		
		МассивТипов.Добавить(Параметр5);
		
	КонецЕсли;
		
	Возврат ?(МассивТипов.Количество()=0,Новый(Тип),Новый(Тип,МассивТипов));
			
КонецФункции // NewObject()

