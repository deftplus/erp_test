	ТекстСебестоимость = СтрЗаменить(ТекстСебестоимость, "&ТипыДокументовПроизводства22", СтрСоединить(ТипыДокументовПроизводства22, ","));
	
	Возврат ТекстСебестоимость;
	
КонецФункции

Функция ТекстПартии() Экспорт
	
	МассивПакетовЗапросов = Новый Массив;
	
	#Область ВТПередачиБезНДС
	ТекстЗапроса = "
		|ВЫБРАТЬ РАЗЛИЧНЫЕ
		|	Т.Ссылка КАК Ссылка,
		|	Т.АналитикаУчетаНоменклатуры.Номенклатура КАК Номенклатура,
		|	Т.АналитикаУчетаНоменклатуры.Характеристика КАК Характеристика,
		|	Т.АналитикаУчетаНоменклатуры.Серия КАК Серия
		|ПОМЕСТИТЬ ВТПередачиБезНДС
		|ИЗ
		|	ДокументыКОтражению КАК ДокументыКОтражению
		|	ВНУТРЕННЕЕ СОЕДИНЕНИЕ Документ.ПередачаТоваровМеждуОрганизациями.ВидыЗапасов КАК Т
		|		ПО Т.Ссылка = ДокументыКОтражению.Ссылка
		|	ВНУТРЕННЕЕ СОЕДИНЕНИЕ Документ.ПередачаТоваровМеждуОрганизациями КАК Передачи
		|		ПО Передачи.Ссылка = ДокументыКОтражению.Ссылка
		|ГДЕ
		|	Передачи.Проведен
		|	И Т.СтавкаНДС = ЗНАЧЕНИЕ(Справочник.СтавкиНДС.БезНДС)
		|ИНДЕКСИРОВАТЬ ПО
		|	Ссылка";
	МассивПакетовЗапросов.Добавить(ТекстЗапроса);
	#КонецОбласти
	
	#Область ВТПередачи
	ТекстЗапроса = "
		|ВЫБРАТЬ
		|	Партии.Регистратор КАК Регистратор,
		|	Партии.НомерСтроки КАК НомерСтроки,
		|	ВЫБОР
		|		КОГДА НЕ ПередачаВидыЗапасов.Ссылка ЕСТЬ NULL И Партии.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Расход)
		|		 И Передача.НалогообложениеНДС = ЗНАЧЕНИЕ(Перечисление.ТипыНалогообложенияНДС.ПродажаОблагаетсяНДС)
		|			ТОГДА ЗНАЧЕНИЕ(Перечисление.ТипыНалогообложенияНДС.ПродажаНеОблагаетсяНДС)
		|		КОГДА НЕ Передача.Ссылка ЕСТЬ NULL И Партии.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Расход)
		|			ТОГДА Передача.НалогообложениеНДС
		|		ИНАЧЕ Партии.КорВидДеятельностиНДС
		|	КОНЕЦ КАК КорВидДеятельностиНДС
		|ПОМЕСТИТЬ ВТПередачи
		|ИЗ
		|	ДокументыКОтражению КАК ДокументыКОтражению
		|	ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрНакопления.СебестоимостьТоваров КАК Партии
		|		ПО Партии.Регистратор = ДокументыКОтражению.Ссылка
		|	ВНУТРЕННЕЕ СОЕДИНЕНИЕ Документ.ПередачаТоваровМеждуОрганизациями КАК Передача
		|		ПО Передача.Ссылка = Партии.Регистратор
		|	ЛЕВОЕ СОЕДИНЕНИЕ ВТПередачиБезНДС КАК ПередачаВидыЗапасов
		|		ПО ПередачаВидыЗапасов.Ссылка = Партии.Регистратор
		|			И ПередачаВидыЗапасов.Номенклатура = Партии.АналитикаУчетаНоменклатуры.Номенклатура
		|			И ПередачаВидыЗапасов.Характеристика = Партии.АналитикаУчетаНоменклатуры.Характеристика
		|			И ПередачаВидыЗапасов.Серия = Партии.АналитикаУчетаНоменклатуры.Серия
		|ИНДЕКСИРОВАТЬ ПО
		|	Регистратор,
		|	НомерСтроки";
	МассивПакетовЗапросов.Добавить(ТекстЗапроса);
	#КонецОбласти
	
	#Область ВТПериодыСписанияНДС
	ТекстЗапроса = "
		|ВЫБРАТЬ РАЗЛИЧНЫЕ
		|	Партии.Регистратор,
		|	Партии.Период,
		|	Партии.Организация КАК Организация
		|ПОМЕСТИТЬ ВТПериодыСписанияНДС
		|ИЗ
		|	ДокументыКОтражению КАК ДокументыКОтражению
		|	ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрНакопления.СебестоимостьТоваров КАК Партии
		|		ПО Партии.Регистратор = ДокументыКОтражению.Ссылка
		|ГДЕ
		|	Партии.ВидДеятельностиНДС В (&ТипыОблагаемыеНДС)
		|	И Партии.КорВидДеятельностиНДС В (&ТипыНеОблагаемыеНДС)
		|ИНДЕКСИРОВАТЬ ПО
		|	Организация";
	МассивПакетовЗапросов.Добавить(ТекстЗапроса);
	#КонецОбласти
	
	#Область ВТПериодыУчетныхПолитик
	ТекстЗапроса = "
		|ВЫБРАТЬ
		|	Т.Регистратор,
		|	Т.Период,
		|	Т.Организация КАК Организация,
		|	МАКСИМУМ(УчетныеПолитики.Период) КАК ПериодПолитики
		|ПОМЕСТИТЬ ВТПериодыУчетныхПолитик
		|ИЗ
		|	ВТПериодыСписанияНДС КАК Т
		|	ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.НастройкиУчетаНДС КАК УчетныеПолитики
		|		ПО Т.Организация = УчетныеПолитики.Организация
		|		И Т.Период >= УчетныеПолитики.Период
		|СГРУППИРОВАТЬ ПО
		|	Т.Регистратор,
		|	Т.Период,
		|	Т.Организация
		|ИНДЕКСИРОВАТЬ ПО
		|	Организация,
		|	ПериодПолитики";
	МассивПакетовЗапросов.Добавить(ТекстЗапроса);
	#КонецОбласти
	
	#Область ВТНастройкиСписанияНДС
	ТекстЗапроса = "
		|ВЫБРАТЬ
		|	Т.Регистратор,
		|	Т.Период КАК Период,
		|	Т.Организация КАК Организация,
		|	УчетныеПолитики.ВариантУчетаНДСПриИзмененииВидаДеятельности,
		|	УчетныеПолитики.СтатьяРасходовНеНДС,
		|	УчетныеПолитики.АналитикаРасходовНеНДС,
		|	УчетныеПолитики.СтатьяРасходовЕНВД,
		|	УчетныеПолитики.АналитикаРасходовЕНВД
		|ПОМЕСТИТЬ ВТНастройкиСписанияНДС
		|ИЗ
		|	ВТПериодыУчетныхПолитик КАК Т
		|	ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.НастройкиУчетаНДС КАК УчетныеПолитики
		|		ПО Т.Организация = УчетныеПолитики.Организация
		|		И Т.ПериодПолитики = УчетныеПолитики.Период
		|ГДЕ
		|	УчетныеПолитики.ВариантУчетаНДСПриИзмененииВидаДеятельности <> ЗНАЧЕНИЕ(Перечисление.ВариантыУчетаНДСПриИзмененииВидаДеятельностиНаНеоблагаемую.ВключатьВСтоимость)
		|	И УчетныеПолитики.СтатьяРасходовСписаниеНДС <> ЗНАЧЕНИЕ(ПланВидовХарактеристик.СтатьиРасходов.ПустаяСсылка)
		|ИНДЕКСИРОВАТЬ ПО
		|	Период,
		|	Регистратор,
		|	Организация";
	МассивПакетовЗапросов.Добавить(ТекстЗапроса);
	#КонецОбласти
	
	#Область Партии_Предварительная
	МассивЗапросаПартий = Новый Массив;
	
	#Область ПартииТоваровОрганизаций
	ТекстЗапроса = "
		|ВЫБРАТЬ
		|	Партии.Регистратор КАК Ссылка,
		|	Партии.Период КАК Период,
		|	Партии.Организация КАК Организация,
