&Вместо("ОбработкаПолученияДанныхВыбора")
Процедура ллл_ОбработкаПолученияДанныхВыбора(ДанныеВыбора, Параметры, СтандартнаяОбработка)
	
	
	// Конилов. https://jira.corp.3l.ru/browse/ERP1C-222
	// По какой-то причине в параметры в поле "Налогообложение НДС" попадает не это значение, а структура данных формы.
	// Вытаскиваем оттуда значение "Налогообложение НДС" и подставляем во входные параметры
	
	
	// Конилов. https://jira.corp.3l.ru/browse/ERP1C-226
	// А иногда во входящих параметрах вообще нет поля "Налогообложение НДС"
	
	
	
	
	
	
	// Начало вставки		
    _НалогобложениеНДС=Перечисления.ТипыНалогообложенияНДС.ПустаяСсылка();
	
	Если Параметры.Свойство("НалогообложениеНДС") тогда
	
		Если ТипЗнч(Параметры.НалогообложениеНДС)=тип("ДанныеФормыСтруктура") тогда
			если Параметры.НалогообложениеНДС.Свойство("НалогообложениеНДС",_НалогобложениеНДС)	 тогда
				Параметры.НалогообложениеНДС=_НалогобложениеНДС;			
				
			КонецЕсли;				
				
		КонецЕсли;	
	
	КонецЕсли;
	// Конец вставки
	
	УчетНДСУП.ПолучитьДанныеВыбораСтавкиНДС(ДанныеВыбора, Параметры, СтандартнаяОбработка);
	
	
	
КонецПроцедуры